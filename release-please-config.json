{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-type": "go",
  "include-component-in-tag": false,
  "prerelease": true,
  "version-file": "./VERSION",
  "prerelease-type": "rc",
  "packages": {
    ".": {
         "extra-files": [
            {
              "type": "yaml",
              "path": "Chart.yaml",
              "jsonpath": "$.appVersion"
            },
            {
              "type": "yaml",
              "path": "Chart.yaml",
              "jsonpath": "$.version"
            },
           "VERSION"
         ]
    }
  }
}
